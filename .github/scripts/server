#!/bin/bash
echo "Node Version $(node -v)"

startWebService() {
    cd /www
    npm i
    bash /scriptservice/serverarc
};
startPyService() {
    cd /scriptservice/python/
    bash /scriptservice/pyarc
    
}


executeServer() {
    startPyService;

    if ! [ -f /env/no-web ]; then
            startWebService;
    fi

}






executeServer